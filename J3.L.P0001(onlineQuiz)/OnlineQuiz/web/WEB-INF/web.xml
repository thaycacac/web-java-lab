<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListQuestionServlet</servlet-name>
        <servlet-class>servlet.ListQuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TakeQuizServlet</servlet-name>
        <servlet-class>servlet.TakeQuizServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckResultServlet</servlet-name>
        <servlet-class>servlet.CheckResultServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newser</servlet-name>
        <servlet-class>servlet.newser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListQuestionServlet</servlet-name>
        <url-pattern>/ListQuestionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TakeQuizServlet</servlet-name>
        <url-pattern>/TakeQuizServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckResultServlet</servlet-name>
        <url-pattern>/CheckResultServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newser</servlet-name>
        <url-pattern>/newser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
